@article{pgill:comcom:2011,
title = {{A Socratic method for validation of measurement-based networking research}},
journal = "Computer Communications ",
volume = "34",
number = "1",
pages = "43 - 53",
year = "2011",
note = "",
issn = "0140-3664",
doi = "http://dx.doi.org/10.1016/j.comcom.2010.09.014",
url = "http://www.sciencedirect.com/science/article/pii/S0140366410004305",
author = "Balachander Krishnamurthy and Walter Willinger and Phillipa Gill and Martin Arlitt",
keywords = "Internet measurement",
keywords = "Topology",
keywords = "Wireless",
keywords = "Web",
keywords = "Online Social Networks ",
abstract = "Nearly three decades of Internet measurement has resulted in large-scale global infrastructures used by an increasing number of researchers. They have examined various Internet properties in areas such as network infrastructure (routers, links), traffic (measurement at packet, flow, and session level) and applications (DNS, Web, P2P, Online Social Networks, etc.) and presented results in diverse venues. Key related topics like security and privacy have also been explored. There is however a lack of clearly articulated standards that reduce the probability of common mistakes made in studies involving measurements, their analysis and modeling. A community-wide effort is likely to foster fidelity in datasets obtained from measurements and reused in subsequent studies. We present a Socratic approach as steps towards a solution to this problem by enumerating a sequence of questions that can be answered relatively quickly by both measurers and reusers of datasets. To illustrate the applicability and appropriateness of the questions we answer them for a number of past and current measurement studies. "
}

